<Window x:Class="X_Manager.License"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:X_Manager" Background="{StaticResource Background_General}"
        mc:Ignorable="d" xmlns:resources="clr-namespace:X_Manager.Properties"
        Title="LICENSE" Height="450" Width="800" WindowStartupLocation="CenterOwner">

	<Grid>
		<ScrollViewer>
			<TextBlock Text="{x:Static resources:Resources.LICENSE}" Foreground="{StaticResource Foreground_Button_Enabled}" Margin="10" TextWrapping="Wrap"/>
		</ScrollViewer>
	</Grid>
</Window>
